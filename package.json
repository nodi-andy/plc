{
  "name": "noditron",
  "description": "Noditron is a platform for creating IoT function block diagrams with a web interface. This platform utilizes the PLC (programmable logic controller) concept to create a user-friendly interface for building IoT projects.",
  "version": "0.1.1",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@hookform/resolvers": "^2.9.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.129",
    "@mui/material": "^5.12.3",
    "@uiball/loaders": "^1.2.6",
    "axios": "^1.1.2",
    "jwt-decode": "^3.1.2",
    "latest-version": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.0",
    "react-icons": "^4.4.0",
    "react-multi-select-component": "^4.3.4",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "recoil": "^0.7.5",
    "recoil-persist": "^4.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.5.3",
    "yup": "^0.32.11",
    "express": "^4.15.4",
    "express-ws": "^5.0.2",
    "http-proxy": "^1.18.1"
  },
  "scripts": {
    "start": "node index.mjs",
    "deploy": "gcloud app deploy",
    "debug": "set PORT=8080 &&  react-scripts start",
    "build": "set \"GENERATE_SOURCEMAP=false\" &&  react-scripts build",
    "buildandstart": "npm run build && npm run start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "compress-create-react-app": "^1.4.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1"
  }
